generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id Int @id @default(autoincrement())
  email String @unique
  startTime DateTime @default(now())
}

model authorizationuser {
  id       Int      @id @default(autoincrement())
  email    String @unique
  password String
  expire   DateTime
}